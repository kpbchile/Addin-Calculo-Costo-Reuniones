{
  "name": "calculadora-costo-reuniones-outlook",
  "version": "1.0.0",
  "description": "Outlook Add-in - Calculadora de Costo de Reuniones para Generadora Metropolitana",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "sideload": "office-addin-dev-certs install && office-addin-debugging start manifest.xml desktop"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "html-webpack-plugin": "^5.6.3",
    "office-addin-debugging": "^5.1.7",
    "office-addin-dev-certs": "^1.13.3",
    "office-addin-manifest": "^1.13.7",
    "webpack": "^5.97.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
